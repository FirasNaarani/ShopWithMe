@model IEnumerable<ShopWithMe.Models.Cart>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<table class="table">
    <tr>
        @foreach (var item in Model)
        {
            <td>
                @Html.ActionLink($"{item.NameCart} {item.Date_Time.ToString("dd/MM/yyyy")}", "Details", new { Id = item.Id }, new { @class = "btn-rounded btn btn-info " })
            </td>
        }
    </tr>
</table>
